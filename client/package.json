{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-mock": "vite --mode test",
    "build": "vue-tsc && vite build",
    "build:test": "vue-tsc --noEmit && vite build --mode test",
    "preview": "vite preview --mode test",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:unit:ci": "vitest run --environment jsdom --root src/",
    "test:e2e": "npm run build:test && start-server-and-test 'vite dev --port 3000 --mode test' :3000 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test 'vite dev --port 3000 --mode test' :3000 'cypress run --e2e --config video=false'"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.6",
    "@vue/tsconfig": "^0.1.3",
    "axios": "^1.2.0",
    "quasar": "^2.10.2",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^4.0.1",
    "@cypress/vue": "^5.0.2",
    "@quasar/vite-plugin": "^1.2.3",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "axios-mock-adapter": "^1.21.2",
    "cypress": "^11.2.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.12",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.3",
    "vue-tsc": "^1.0.9"
  }
}
